#!/bin/bash

dev () {
    cd "$HOME/src/github.com/nicheinc/development/$1"
    clear
}

_dev () {
    local cur="${COMP_WORDS[COMP_CWORD]}"
    if [[ $cur = /* ]] || [[ $cur = ~* ]]; then
        return
    fi
    cd "$HOME/development"
    compopt -o filenames 2>/dev/null
    COMPREPLY=( $(compgen -d -- "$cur"))
}

complete -F _dev dev
